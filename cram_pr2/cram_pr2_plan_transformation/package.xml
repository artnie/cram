<?xml version="1.0"?>
<package format="2">
  <name>plan_transformation</name>
  <version>0.0.1</version>
  <description>
    The plan_transformation package takes cram-task-trees and modifies them by applying specific rules on them.
    The purpose of this modification is to optimize plans by slightly altering the sequence of executed plans.
  </description>
  <author>Arthur Niedzwiecki, Gayane Kazhoyan</author>
  <maintainer email="arthur.niedzwiecki@gmail.com">Arthur Niedzwiecki</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>alexandria</build_depend>
  <build_depend>cram_language</build_depend>
  <build_depend>roslisp</build_depend>
  <build_export_depend>alexandria</build_export_depend>
  <build_export_depend>cram_language</build_export_depend>
  <build_export_depend>roslisp</build_export_depend>
  <exec_depend>alexandria</exec_depend>
  <exec_depend>cram_language</exec_depend>
  <exec_depend>roslisp</exec_depend>


